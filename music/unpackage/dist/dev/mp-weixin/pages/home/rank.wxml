<view class="RankPage"><cu-custom vue-id="79248633-1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">歌单广场</view></cu-custom><view class="rankSheet"><view class="rankTitle">官方榜</view><block wx:for="{{rankLists}}" wx:for-item="item" wx:for-index="index" wx:key="id"><block wx:if="{{item.tracks&&item.tracks.length}}"><view data-event-opts="{{[['tap',[['handleClick',['$0','$1'],[[['rankLists','id',item.id,'id']],[['rankLists','id',item.id,'name']]]]]]]}}" class="rank-song-sheet" bindtap="__e"><view class="rank-song-item"><view class="rank-pic"><image mode="aspectFill" src="{{item.coverImgUrl}}"></image><view class="tips">{{item.updateFrequency}}</view></view><view class="rank-name-list"><block wx:for="{{item.tracks}}" wx:for-item="track" wx:for-index="__i0__" wx:key="second"><view class="song-name">{{''+track.first+" - "+track.second+''}}</view></block></view></view></view></block></block></view></view>